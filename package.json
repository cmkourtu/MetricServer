{
  "name": "app-name",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./app",
    "start": "node ./app",
    "migrate": "npx sequelize db:migrate && npx sequelize db:seed:all",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "seed:undo": "npx sequelize db:seed:undo",
    "migration": "npx sequelize migration:generate --name",
    "seed": "npx sequelize seed:generate --name",
    "model": "npx sequelize model:create --name",
    "scaffold": "npx yo ./_generate.js",
    "prod": "yarn prod:delete; yarn prod:start",
    "prod:start": "NODE_ENV=production pm2 start ./app.js --name app-name",
    "prod:restart": "NODE_ENV=production pm2 restart app-name",
    "prod:delete": "pm2 delete app-name",
    "prod:migrate": "NODE_ENV=production npx sequelize db:migrate && npx sequelize db:seed:all",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-random-string": "^3.3.1",
    "debug": "^4.1.1",
    "dotenv-flow": "^3.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.6.7",
    "express-validator": "^6.2.0",
    "generate-password": "^1.4.2",
    "http-errors": "^1.8.0",
    "hybrid-crypto-js": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "moment": "^2.25.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pluralize": "^8.0.0",
    "rand-token": "^0.4.0",
    "response-time": "^2.3.2",
    "sequelize": "^5.19.6",
    "yeoman-generator": "^4.10.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.22.0"
  }
}

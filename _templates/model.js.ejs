module.exports = (sequelize, DataTypes) => {
  /**
   * @typedef <%= modelName.camelCase %>
   * @property {integer} id<% for (attributeName in modelAttibutes) { %>
   * @property {<%= modelAttibutes[attributeName] %>} <%= attributeName %><% } %>
   * @property {integer} createdAt
   * @property {integer} updatedAt
   */
  const <%= modelName.camelCase %> = sequelize.define(
    '<%= modelName.camelCase %>',
    {<% for (attributeName in modelAttibutes) { %>
      <%= attributeName %>: {
        type: DataTypes.<%= _.toUpper(modelAttibutes[attributeName]) %>,
      },<% } %>
    },
    {}
  );
  <%= modelName.camelCase %>.associate = function() {
    // associations can be defined here
  };

  return <%= modelName.camelCase %>;
};
